@model IEnumerable<Sky_Roamer.Models.Tour_Package>

@{
    ViewBag.Title = "View_Package";
}

<h2>View Package</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PackageId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Package_Name)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.PackageId.ToString(), "Details", new { id = item.PackageId })
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Package_Name)
            </td>
        </tr>
    }

</table>
@Html.ActionLink("Go to Profile", "CustomerProfile", new { value = @Session["Customer"] }) |
@Html.ActionLink("Logout", "Logout", "Home")
